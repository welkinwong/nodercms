      <div class="row">
        {{#foreach lists}}
          <div class="col-md-6">
            <div class="panel panel-less">
              <div class="panel-heading">
                {{name}}
                <a class="pull-right" href="{{path}}">更多</a>
              </div>
              <ul class="list-group list-group-less">
                {{#foreach contents limit="5"}}
